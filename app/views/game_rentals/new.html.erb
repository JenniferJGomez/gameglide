<% @game_rental.errors.full_messages.each do |msg|%>
<li><%= msg %></li>
<% end %> 
<h1> Choose your Rental </h1>

<div class="new-form">
<%= form_for @game_rental do |f|%>
    <%= f.collection_select(:user_id, @user, :id, :username, prompt: "Select User") %>
    <%= f.collection_select(:game_id, Game.all, :id, :title, prompt: "Select Game") %>
    <%= f.submit "Rent Game"%>
<%end%>
</div>
    