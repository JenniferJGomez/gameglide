<h1>Welcome Back to GameGlide!</h1>
<h3>Login:</h3>
<% if flash.notice %>
    <div class="error"><%= flash.notice %></div>
<% end %>
 <%=form_tag '/login' do  %>
 <%= label :username, "Username" %>
 <%=text_field_tag :"user[username]"%>
 <%= label :password, "Password" %>
 <%=password_field_tag :"user[password]"%>
 <%=submit_tag :submit%>
 <%end%><br/>
 <%= link_to "Back to Sign Up Page",'/signup'%>

